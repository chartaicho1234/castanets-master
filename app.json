{
  "expo": {
    "name": "カスタネットマスター",
    "slug": "castanets-master",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "baseUrl": "/castanets-master/"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "/castanets-master/"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
